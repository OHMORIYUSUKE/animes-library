// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./anime.db"
}

model Anime {
  id                Int      @id
  year              Int
  cool              Int
  title             String
  title_short1      String?
  title_short2      String?
  title_short3      String?
  title_en          String?
  public_url        String
  twitter_account   String
  twitter_hash_tag  String?
  cours_id          Int?
  created_at        DateTime
  updated_at        DateTime
  sex               Int
  sequel            Int
  city_code         Int
  city_name         String
  product_companies String?
  ogp_description   String?
  ogp_image_url     String
}
